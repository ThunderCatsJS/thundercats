{
  "name": "todomvc-flux",
  "version": "0.0.3",
  "description": "Example Flux architecture.",
  "repository": "https://github.com/facebook/flux",
  "main": "js/app.js",
  "dependencies": {
    "keymirror": "~0.1.1",
    "object-assign": "^2.0.0",
    "react": "^0.13.1",
    "rx": "^2.4.9",
    "thundercats": "^1.5.0"
  },
  "devDependencies": {
    "babelify": "^6.0.1",
    "browser-sync": "^2.5.3",
    "browserify": "^9.0.6",
    "envify": "^3.4.0",
    "jest-cli": "~0.4.0",
    "uglify-js": "~2.4.19",
    "watchify": "^3.1.0"
  },
  "scripts": {
    "start": "watchify js/app.js -d -o js/bundle.js -v & browser-sync start --server --files js/bundle.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "author": "Bill Fisher",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
